<div>
  {{#with main}}
  <main>
    
    {{> title data="My educations" big=true}}
    
    <section>
     
      <ul>
        {{#each educations}}
        <li>
          {{name}}
          
          {{#unless isEnd includeZero=true}} (ще в процесі) {{/unless}}
         
        </li>
        {{/each}}
      </ul>
    
    </section>

    {{> title data="My certificates" big=false}}

    <section>
       
        <ul>
          {{#each certificates}}
          <li>
            {{name}} ({{id}})
          </li>
          {{/each}}
        </ul>

    </section>


  </main>
  {{/with}}
</div>