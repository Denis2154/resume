<div class='text-center'>
  {{#with page_header}}
    <h1>{{title}}</h1>
    <p class='text-uppercase'>{{description}}</p>
  {{/with}}
</div>
<section>
  <div class='card'>
    <div class='card-body'>
      {{#with section}}
        {{#with about_us}}
          {{#with title_list}}
            <div class='text-end display-6'>{{main}}</div>
            <br />
            <h2 class='text-end'>{{sub}}</h2>
          {{/with}}
          <button
            class='btn btn-info'
          >{{button.text}}</button>
        {{/with}}
    </div>
  </div>
</section>
      
     <section>
        <div class="card">
            <div class="card-body">
             {{#with advantage}}
              <div class="display-5">{{title}}</div>
              <br>
              <div class="card">
                <div class="card-body">
                    {{#with efficiency}}
                      <div class="display-4">{{title}}</div>
                      <br>
                      <h6>{{sub_title}}</h6>
                      <br>
                      <p>{{description}}</p>
                      <button class="btn btn-warning">{{button}}</button>
                   {{/with}}
                </div>
             </div>
             <br>
             <div class="card">
                <div class="card-body">
                   {{#with innovation}}
                      <div class="display-4 text-center">{{title}}</div>
                      <br>
                      <h6 class="text-center">{{sub_title}}</h6>
                      <br>
                      <p class="text-center">{{description}}</p>
                      <button class="btn btn-warning">{{button}}</button>
                   {{/with}}
                </div>
             </div>
             <br>
             <div class="card">
                <div class="card-body">
                    {{#with collaboration}}
                      <div class="display-4 text-end">{{title}}</div>
                      <br>
                      <h6 class="text-end">{{sub_title}}</h6>
                      <br>
                      <p class="text-end">{{description}}</p>
                      <button class="btn btn-warning">{{button}}</button>
                    {{/with}}
                </div>
             </div>
             
             {{/with}}

            </div>
        </div>
     </section> 
     
     <section>
        <div class="card">
            <div class="card-body">
                {{#with agreement}}
                 <div class="display-5">{{title}}</div>
                 <div class="card">
                    <div class="card-body">
                        {{#with info}}
                        <h5>{{sub_title}}</h5>
                        <div class="card">
                            <div class="card-body">
                                {{#with detail}}
                                 <p>{{text}}</p>
                                 <button class="btn btn-success">{{button}}</button>
                                 {{/with}}
                            </div>
                        </div>
                        {{/with}}
                    </div>
                 </div>
                {{/with}}
            </div>
        </div>
     </section> 



  {{/with}}




